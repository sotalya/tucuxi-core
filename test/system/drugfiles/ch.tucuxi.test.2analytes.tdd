                                                    MALE = sex;

                                                    TVCL = theta1 + theta4 * FBW + theta5 * MALE-theta5 * (1-MALE) + theta6 * FAGE + theta7 * PATH - theta7 * (1 - PATH);

                                                    return TVCL;
                                                                             ]]>
                                                </code>
                                            </softFormula>
                                            <comments/>
                                        </aprioriComputation>
                                    </parameterValue>
                                    <bsv>
                                        <bsvType>exponential</bsvType>
                                        <stdDevs>
                                            <stdDev>0.356</stdDev>
                                        </stdDevs>
                                    </bsv>
                                    <validation>
                                        <errorMessage>
                                            <text lang='fr'>Clearance shall be in the range [0, 300].</text>
                                        </errorMessage>
                                        <operation>
                                            <softFormula>
                                                <inputs>
                                                    <input>
                                                        <id>CL</id>
                                                        <type>double</type>
                                                    </input>
                                                </inputs>
                                                <code>
                                                    <![CDATA[
                                                    return CL < 300.0 and CL > 0.0;
                                                                ]]>
                                                    </code>
                                                </softFormula>
                                                <comments/>
                                            </operation>
                                            <comments/>
