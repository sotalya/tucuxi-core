                                                        <id>sex</id>
                                                        <type>double</type>
                                                    </input>
                                                </inputs>
                                                <code>
                                                    <![CDATA[
                                                    theta1 = CL_population;
                                                    theta4 = 5.42;
                                                    theta5 = 1.49;
                                                    theta6 = -5.81;
                                                    theta7 = -0.806;

                                                    MEANBW = 70;
                                                    FBW = (bodyweight - MEANBW) / MEANBW;

                                                    MEANAG = 50;
                                                    FAGE = (age - MEANAG) / MEANAG;

                                                    if (gist)
                                                      PATH = 1;
